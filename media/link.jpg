        function handleApiError(error_message) {
            if(!$('.notice-error').length) {
                $('#page_container').prepend("<div class='notice notice-error'>" +
                    "<span class='close-notice pull-right' title=\"Close This Notification!\">" +
                        "<i class=\"el el-remove\"></i>" +
                    "</span>" +
                    "<b></b>" +
                    "</div>")
            }

            $('.notice-error b').html(error_message);
            $('html,body').animate({scrollTop: $('.notice-error').offset().top - 15});
        }
    </script>

        <script>
        let sorting_form_loaded = 0;

        $("#load_sorting_form").on("click", function() {
            //Form is already loaded, just toggle
            if(sorting_form_loaded) {
                $('#sorting_options').collapse("toggle");
                return;
            }

            $('body').addClass('waiting');

            $.post("/sorting-form.php", {
                show_resolutions: '1',
                                show_sorting: '1'
                            },
            function(data) {
                $('#sorting_options').html(data);
            }).done(function() {
                render_sorting_form();
                $('body').removeClass('waiting');
            });
        });

        function render_sorting_form() {
            const ratio = window.devicePixelRatio || 1;
            let width, height;

            if(ratio == 1) {
                width = window.screen.width;
                height = window.screen.height;
            }
            else {
                width = Math.floor(window.screen.width * ratio);
                height = Math.floor(window.screen.height * ratio);
            }

            if(parseInt(width.toString().split('').pop()) == 1 && parseInt(height.toString().split('').pop()) == 1) {
                width -= 1;
                height -= 1;
            }
            //Apple iPhone 6+, 6s+, 7+, 8+
            else if(width == 1242 && height == 2208 && ratio == 3)
            {
                width = 1080;
                height = 1920;
            }

            //Add the screen resolution to the list
            $('select[name=min_resolution] option').first().after('<option value="'+width+'x'+height+'" data-subtext="Your Resolution">'+width+'x'+height+'</option>');
            if( $("select[name=min_resolution] option[value='0x0']").length == 0 ) {
                $('select[name=min_resolution]').append('<option value="0x0" selected>0x0</option>');
            }
            else {
                $('select[name=min_resolution]').val('0x0');
            }

            //Transform each sorting select into an dropdown
            $('.sorting-select select').each(function() {

                var listElement = "";
                var subtext;
                var option;
                var select = $(this);

                select.after('<div class="sorting-select-dropdown"></div>');
                var dropdown = select.next();
                dropdown.addClass(select.attr('data-class'));

                if(select.attr('disabled') !== undefined)
                    dropdown.append('<button type="button" class="btn dropdown-toggle disabled"><span class="sorting-btn-select">'+$("option:selected", this).text()+'</span>&nbsp;<i class="el el-chevron-down"></i></button>');
                else {
                    dropdown.append('<button aria-expanded="false" type="button" class="btn dropdown-toggle" data-toggle="dropdown"><span class="sorting-btn-select">'+$("option:selected", this).text()+'</span>&nbsp;<i class="el el-chevron-down"></i></button>');

                    select.children().each(function() {
                        option = $(this);

                        subtext = "";
                        if(option.attr('data-subtext') !== undefined)
                            subtext = '<span class="subtext">'+option.attr('data-subtext')+'</span>';

                        if( option.is(':selected') )
                            listElement += '<li data-value="'+option.val()+'" class="select-selected"><a><span class="sorting-select-text"><span class="dropdown-content">'+option.text()+'</span>'+subtext+'</span><i class="el el-ok sorting-tick"></i></a></li>';
                        else
                            listElement += '<li data-value="'+option.val()+'"><a><span class="sorting-select-text"><span class="dropdown-content">'+option.text()+'</span>'+subtext+'</span><i class="el el-ok sorting-tick"></i></a></li>';
                    });
                    dropdown.append('<div class="dropdown-menu open"><div class="popover-title">'+select.attr('data-header')+'</div><ul class="dropdown-menu inner" role="menu">'+listElement+'</ul></div>');
                }
            });

            //Manage the click on a 'select' dropdown, update css and the 'html select'
            $('.sorting-select-dropdown').on('click', 'li', function(event) {
                if( !$(this).hasClass('select-selected') ) {
                    $(this).closest('.sorting-select-dropdown').find('.sorting-btn-select').html($(this).find('.dropdown-content').html());
                    $(this).closest('.sorting-select').find('select').val($(this).attr('data-value'));
                    $(this).parent().find('.select-selected').removeClass('select-selected');
                    $(this).addClass('select-selected');
                }
            });

            // Add the right icon according to checked or unchecked
            $('.sorting-selector').each(function () {
                var element = $(this);

                if( element.find('input').is(':checked') )
                    element.append("<i class='el el-check sorting-icon'></i>");
                else
                    element.append("<i class='el el-check-empty sorting-icon'></i>");
            });

            // Another "radio" element is selected
            $('.sorting-selector').click(function () {
                var element = $(this);
                var input = element.find('input');
                if( !input.is(':checked') ) {
                    $('input[name='+input.attr('name')+']:checked').next().toggleClass('el-check el-check-empty')
                    input.prop("checked", true);
                    element.find('.sorting-icon').toggleClass('el-check-empty el-check');
                }
            });

            sorting_form_loaded = 1;

            //The form is ready, show it
            $('#sorting_options').collapse();
        }
    </script>

    <script>
        let is_loading = 0;
        let loading_more_next = 0;
                const max_displayed = 8;
                let current_page = 1;
        let next_page = 2;
        // Last page is either set, or defaults to 0, in which case we don't know what the last page is, as we didn't get a count on the page.
                    let last_page = 2215;
                
        let clean_timer;

        const title_next_disabled = "Auto loading the next pages is disabled!";
        const title_next_enabled = "Auto loading the next pages is enabled!";

        //Load the next page of results
        function loadingNext() {
            $('#loader_next').show();

            let next_url_base = "https://wall.alphacoders.com/by_category.php?id=10&name=Earth+Wallpapers";
            let next_url_params = "";

            // ? or & based on if there's already content in the query bit of the url
                            next_url_params += "&";
            
                            next_url_params += "quickload=66431&page=" + next_page; 
            
            // We're splitting up the single url into next_url_base and next_url_params (all the info for quick load) so that Google doesn't crawl the 
            // quickload pages. I'm not sure if it would still be an issue with params setup the way they are now, but it doesn't hurt.
            $.get(next_url_base + next_url_params,
                function(data) {

                    // Lets try and see if our next page returned any wallpapers
                    let next_page_returned_wallpapers = false;

                    if(data.search("thumb-container") !== -1) {
                        next_page_returned_wallpapers = true;
                    } 

                    if(next_page_returned_wallpapers) {
                        //Insert the new results
                        $("#loader_next").before(data);
                    } else {
                        // not going to need to keep grabbing pages
                        last_page = next_page;
                    }


                }
            ).done(function() {
                $('#loader_next').hide();
                next_page++;




                $(".big-thumb").one("load", function() {
                    var container = $(this).closest('.thumb-container-big');

                    $(".boxcaption", container).stop().css('top', (container.height() - $(".thumb-info", container).height() - 4) + "px");
                    $(this).removeClass('big-thumb');
                }).each( function() {
                    if(this.complete) {
                        $(this).trigger('load');
                    }
                });

                //Set the field "auto load" to the proper value, by default next is enabled
                if(loading_more_next == 0) {
                    $('.auto-load-next:last').html("<i class=\"el el-remove\"></i>");
                    $('.auto-load-button-next:last').prop('title', title_next_disabled);
                }

                //Is there more "next" pages
                if(next_page > last_page || last_page === 0) {
                    $('#next_button').hide();
                }
                else {
                    $('#span_next_page').html(next_page);
                }

                setTimeout(function () {
                    is_loading = 0;
                }, 1000);
            });
        }

        //If the page represent at least 2/5 of the page, used to update the url
        function mostlyVisible(element) {
            let box = element.getBoundingClientRect();
            let window_height = $(window).height();
            return ( box.top < ( window_height * (3/5) ) );
        }

        //Delete elements to free space/memory
        function deleteElement() {
            let containers = $(".page_container");

            if(containers.length > max_displayed) {
                containers.slice(0, (containers.length-max_displayed)).each(function(){
                    const final_position =  $(window).scrollTop() - ($(this).height() + 10);

                    //Remove the container
                    $(this).css('height', $(this).height()).html("").remove();

                    //Update scroll position
                    if($(window).scrollTop() > (final_position * 1.05) || $(window).scrollTop() < (final_position * 0.95))
                    {
                        $(window).scrollTop(final_position);
                    }
                });
            }
        }

        function scrollToTop() {
            document.body.scrollTop = 0;
            document.documentElement.scrollTop = 0;
        }

        function toggleNextPageAutoLoad() {
            if( loading_more_next == 1 ) {
                loading_more_next = 0;
                $('.auto-load-next').html("<i class='el el-remove'></i>");
                $('.auto-load-button-next').prop('title', title_next_disabled);
                if(next_page < last_page || last_page === 0)
                    $('#next_button').show();
            }
            else {
                loading_more_next = 1;
                $('.auto-load-next').html("<i class='el el-ok'></i>");
                $('.auto-load-button-next').prop('title', title_next_enabled);
                $('#next_button').hide();
            }
        }

        //Load manually the next page of results
        $('#next_button').on("click", function() {
            if( next_page <= last_page || last_page === 0) {
                is_loading = 1;
                loadingNext();
            }
        });

        $( document ).ready(function() {

            // handle scroll events to update content
            $(window).scroll(function() {
                const scroll_pos = $(window).scrollTop();

                //Load next page
                if( is_loading == 0 && loading_more_next == 1 && (next_page <= last_page || last_page === 0)
                    && ( scroll_pos >= ( $('#big_container_bottom').position().top - $(window).height() - 400 ) )
                    && scroll_pos < (  $('#big_container_bottom').position().top + 200 ) )
                {
                    is_loading = 1;
                    loadingNext();
                }

                clearTimeout(clean_timer);
                clean_timer = setTimeout(function () {
                    deleteElement();
                }, 750);
            });

            setTimeout(function () {
                window.scrollTo(0,0);
                loading_more_next = 1;
            }, 500);
        });
    </script>
